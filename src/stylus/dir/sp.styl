#home
 background-color
//

.clearfix
  &:after
    content ' '
    display  block
    clear both
.Header
  // height (37*mn)
  padding-top (60*mn)
  margin-bottom (50*mn)
  position relative

  &__ttl
    font-size (20*mn)
    position absolute
    left 0
    right 0
    text-align center
  &__vol
    font-style italic
    font-size (12*mn)
    float left
    margin-left (43*mn)
    margin-top (13*mn)
    border-bottom solid black
  &__btn
    width (49*mn)
    height (37*mn)
    float right
    margin-right (51*mn)

.mv
  position relative
  margin-top (87*mn)
  &__thema
    width (485*mn)
    height (319*mn)
    position absolute
    left (10*mn)
    top (-8*mn)
  &__issue
    width (270*mn)
    position absolute
    top (745*mn)
    left (503*mn)
  &__subthema
    width (646*mn)
    position absolute
    left 54px
    // margin auto がきかない
    top (552*mn)
    z-index 1
  &__line
    width (750*mn)
    position absolute
    top (639*mn)
    z-index 1
  &__midhight
    width (216*mn)
    position absolute
    top (780*mn)
    left (54*mn)

.text
  margin-top (36*mn)
  position relative
  &__logo
    width (75*mn)
    height (59*mn)
    position absolute
    top (43*mn)
    right (152*mn)
    bottom (86*mn)
    left (64*mn)
  &__text
    float right
    font-size (25*mn)
    line-height (51*mn)
    margin-top (122*mn)
    padding-left (30*mn)
    padding-right (30*mn)


